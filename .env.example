# API Keys for JailbreakingLLMs
# Copy this file to .env and add your actual API keys

# OpenAI API Key (for GPT-3.5 and GPT-4)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Gemini API Key (for Gemini Pro model)
GEMINI_API_KEY=your_gemini_api_key_here

# Together AI API Key (for Vicuna, Llama-2, and Mixtral models)
TOGETHER_API_KEY=your_together_api_key_here

# WandB API Key (optional, for experiment logging)
WANDB_API_KEY=your_wandb_api_key_here
